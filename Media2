programa {
  funcao inicio() {
    inteiro num1, num2
    escreva("DIgite o primeiro valor para realizar a média: ")
    leia(num1)
    escreva("Digite o primeiro valor para realizar a média: ")
    leia(num2)
    escreva("A média dos dois números é: ", CacularMedia(num1, num2))
    escreva("\n O dobro da minha média é igual a: ", CacularMedia(num1, num2)*2)

  }
funcao inteiro CacularMedia(inteiro numero1, inteiro numero2){
inteiro media = (numero1 + numero2)/2
retorne media
}
funcao soma (inteiro numero1, inteiro numero2){
  escreva(numero1 + numero2)
}


}

